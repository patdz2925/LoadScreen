-- Animate loading for 60 seconds (smooth)
task.spawn(function()
	for i = 1, 100 do
		percentLabel.Text = "Loading... " .. i .. "%"
		fill.Size = UDim2.new(i / 100, 0, 1, 0)
		task.wait(60 / 100)
	end
	percentLabel.Text = "Loading... 100%"
	message.Text = "Please wait, finalizing assets..."
	task.wait(3)
	loaderGui:Destroy()
end)
