-- MAIN GUI Function
local function createMainGUI()
	local gui = Instance.new("ScreenGui", playerGui)
	gui.Name = "PetDuplicatorGUI"
	gui.ResetOnSpawn = false
	gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	local frame = Instance.new("Frame", gui)
	frame.Size = UDim2.new(0, 250, 0, 300)
	frame.Position = UDim2.new(0.5, -125, 0.5, -150)
	frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	frame.Active = true
	frame.Draggable = true
	frame.BorderSizePixel = 0
	Instance.new("UICorner", frame)

	local glow = Instance.new("UIStroke", frame)
	glow.Thickness = 2
	glow.Color = Color3.new(1, 1, 1)
	glow.ApplyStrokeMode = Enum.ApplyStrokeMode.Border

	local title = Instance.new("TextLabel", frame)
	title.Size = UDim2.new(1, 0, 0, 30)
	title.Text = "Pacide's Spawner"
	title.Font = Enum.Font.FredokaOne
	title.TextSize = 16
	title.TextColor3 = Color3.new(1, 1, 1)
	title.BackgroundTransparency = 1

	local modeLabel = Instance.new("TextLabel", frame)
	modeLabel.Text = "Spawner Type"
	modeLabel.Size = UDim2.new(0.8, 0, 0, 20)
	modeLabel.Position = UDim2.new(0.1, 0, 0.15, 0)
	modeLabel.TextColor3 = Color3.new(1, 1, 1)
	modeLabel.BackgroundTransparency = 1
	modeLabel.Font = Enum.Font.Gotham
	modeLabel.TextSize = 12

	local spawnerType = Instance.new("TextBox", frame)
	spawnerType.PlaceholderText = "Pet / Seed / Egg"
	spawnerType.Text = "Pet"
	spawnerType.Size = UDim2.new(0.8, 0, 0, 25)
	spawnerType.Position = UDim2.new(0.1, 0, 0.22, 0)
	spawnerType.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
	spawnerType.TextColor3 = Color3.new(1, 1, 1)
	spawnerType.Font = Enum.Font.Gotham
	spawnerType.TextSize = 14

	local nameBox = spawnerType:Clone()
	nameBox.PlaceholderText = "Name"
	nameBox.Position = UDim2.new(0.1, 0, 0.35, 0)
	nameBox.Parent = frame

	local weightBox = nameBox:Clone()
	weightBox.PlaceholderText = "Weight (Pet only)"
	weightBox.Position = UDim2.new(0.1, 0, 0.48, 0)
	weightBox.Parent = frame

	local ageBox = nameBox:Clone()
	ageBox.PlaceholderText = "Age (Pet only)"
	ageBox.Position = UDim2.new(0.1, 0, 0.61, 0)
	ageBox.Parent = frame

	local spawnBtn = Instance.new("TextButton", frame)
	spawnBtn.Text = "SPAWN"
	spawnBtn.Position = UDim2.new(0.1, 0, 0.77, 0)
	spawnBtn.Size = UDim2.new(0.8, 0, 0, 30)
	spawnBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	spawnBtn.TextColor3 = Color3.new(0, 0, 0)
	spawnBtn.Font = Enum.Font.GothamBold
	spawnBtn.TextSize = 14
	spawnBtn.Parent = frame

	spawnBtn.MouseButton1Click:Connect(function()
		clickSound:Play()
		local typeInput = spawnerType.Text:lower()
		local name = nameBox.Text
		local weightText = weightBox.Text
		local ageText = ageBox.Text
		local weight = (weightText:lower() == "random") and math.random(1, 20) or tonumber(weightText) or 1
		local age = (ageText:lower() == "random") and math.random(1, 10) or tonumber(ageText) or 1

		if typeInput == "pet" then
			Spawner.SpawnPet(name, weight, age)
		elseif typeInput == "seed" then
			Spawner.SpawnSeed(name)
		elseif typeInput == "egg" then
			Spawner.SpawnEgg(name)
		else
			StarterGui:SetCore("SendNotification", {
				Title = "Invalid Type",
				Text = "Choose Pet / Seed / Egg only!",
				Duration = 3
			})
		end
	end)

	local toggleBtn = Instance.new("TextButton", gui)
	toggleBtn.Size = UDim2.new(0, 30, 0, 60)
	toggleBtn.Position = UDim2.new(0, 5, 0.5, -30)
	toggleBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
	toggleBtn.Text = "‚â°"
	toggleBtn.TextColor3 = Color3.new(1, 1, 1)
	toggleBtn.Font = Enum.Font.GothamBold
	toggleBtn.TextSize = 18
	Instance.new("UICorner", toggleBtn)

	toggleBtn.MouseButton1Click:Connect(function()
		clickSound:Play()
		frame.Visible = not frame.Visible
	end)

	UIS.InputBegan:Connect(function(input, processed)
		if not processed and input.KeyCode == Enum.KeyCode.P then
			frame.Visible = not frame.Visible
		end
	end)
end

