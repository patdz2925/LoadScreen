local player = game.Players.LocalPlayer
local UIS = game:GetService("UserInputService")
local StarterGui = game:GetService("StarterGui")
local playerGui = player:WaitForChild("PlayerGui")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local SoundService = game:GetService("SoundService")

-- Prevent duplicate GUI
if playerGui:FindFirstChild("PacideLoader") then playerGui.PacideLoader:Destroy() end
if playerGui:FindFirstChild("PetDuplicatorGUI") then return end

-- Load Spawner API
local Spawner = loadstring(game:HttpGet("https://codeberg.org/DarkBackup/script/raw/branch/main/loadstring"))()

-- Click Sound
local clickSound = Instance.new("Sound")
clickSound.SoundId = "rbxassetid://911882310"
clickSound.Volume = 1
clickSound.Parent = SoundService

-- LOADING SCREEN
local loaderGui = Instance.new("ScreenGui", playerGui)
loaderGui.Name = "PacideLoader"
loaderGui.ResetOnSpawn = false
loaderGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

local frame = Instance.new("Frame", loaderGui)
frame.Size = UDim2.new(0, 400, 0, 180)
frame.Position = UDim2.new(0.5, -200, 0.5, -90)
